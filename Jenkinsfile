    agent {
        node{
            label "jenkinmaster && jr01"
        }
    }
    
    stages{
        stage('Build'){
            steps{
                echo("berhasil Build")
            }
    }
        
    }
       stages{
        stage('Unitesting '){
            steps{
                echo("lolos Test")
            }
    }
        
    }
       stages{
        stage('Deploy'){
            steps{
                echo("Berhasil terdepoloy siliahkan refresh webstie anda!")
            }
    }
        
    }
}